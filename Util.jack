class Util {
    // >=
    function boolean ge(int a, int b) {
        if(a<b) {
            return false;
        }
        return true;
    }

    // <=
    function boolean le(int a, int b) {
        if(a > b) { 
            return false;
        }
        return true;
    }

    // !=, ~=
    function boolean ne(int a, int b) {
        if(a = b) { 
            return false;
        }
        return true;
    }

    // %
    function int rem(int a, int b) {
        return a-b*(a/b);
    }
}