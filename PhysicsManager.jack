class PhysicsManager {
    static Pikachu p;
    static PikachuComputer pc;
    static Ball b;
    static Rod r;
    static Floor f;

    function void init(Pikachu pi, PikachuComputer pco, Ball ba, Rod ro, Floor fl) {
        let p = pi;
        let pc = pco;
        let b = ba;
        let r = ro;
        let f = fl;
        return;
    }

    function void update() {
        do p.update();
        do pc.update();
        do r.update();
        do b.update();
        do f.update();
        return;
    }

    function Ball getBall() {
        return b;
    }

    function Rod getRod() {
        return r;
    }

    function Floor getFloor() {
        return f;
    }
}