class PhysicsManager {
    static Pikachu p;
    static Ball b;

    function void init(Pikachu pi, Ball ba) {
        let p = pi;
        let b = ba;
        return;
    }

    function void update() {
        do p.update();
        do b.update();
        return;
    }

    function Ball getBall() {
        return b;
    }
}