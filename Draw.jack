    function void drawBall(int x, int y, int t) {
        if (t < 5) {
            do Screen.drawCircle(x, y, 30);
            do Screen.setColor(0);
            do Screen.drawCircle(x, y, 10);
            do Screen.drawRectangle(x-28, y-3, x+28, y+3);
            do Screen.setColor(1);
            do Screen.drawCircle(x, y, 5);
        }
        else{
            do Screen.drawCircle(x, y, 30);
            do Screen.setColor(0);
            do Screen.drawCircle(x, y, 10);
            do Screen.drawRectangle(x-3, y-28, x+3, y+28);
            do Screen.setColor(1);
            do Screen.drawCircle(x, y, 5);
        }
        return;
    }

