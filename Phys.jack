class Phys {
    static Float dt, g;

    function void init() {
        let dt = Float.new();
        do dt.setInt(1);
        do dt.divInt(1000); // dt = 0.001
        let g = Float.new();
        do g.setInt(98);
        do g.divInt(10); // g = 9.8
        return;
    }

    function Float getDt() {
        return dt;
    }

    function Float getG() {
        return g;
    }
}